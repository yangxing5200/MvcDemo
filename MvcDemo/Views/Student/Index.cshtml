@model IEnumerable<Entity.STUDENT>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Name
        </th>
        <th>
            SchoolId
        </th>
        <th>
            JoinDate
        </th>
        <th>
            GarduationDate
        </th>
        <th>
            Sex
        </th>
        <th>
            Creator
        </th>
        <th>
            CreateDate
        </th>
        <th>
            Modify
        </th>
        <th>
            ModifyDate
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
